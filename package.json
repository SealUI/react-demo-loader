{
  "name": "react-demo-loader",
  "version": "0.5.0",
  "description": "webpack loader for react demo",
  "main": "/lib/index.js",
  "scripts": {
    "test": " cross-env NODE_ENV=test ava",
    "build": "babel src --out-dir lib",
    "precommit": "npm run build && lint-staged && git add -A"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write --tab-width 2 --no-semi",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@babel/template": "^7.2.2",
    "@babel/types": "^7.2.2",
    "chalk": "^2.3.2",
    "detab": "^2.0.1",
    "fs-extra": "^5.0.0",
    "loader-utils": "^1.2.1",
    "lodash.get": "^4.4.2",
    "mdast-util-to-hast": "^4.0.0",
    "nanoid": "^2.0.0",
    "remark-highlight.js": "^5.1.0",
    "remark-parse": "^6.0.3",
    "schema-utils": "^1.0.0",
    "unified": "^7.1.0",
    "unist-builder": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.11.x",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "webpack": "^4.28.2"
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.spec.js"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "loose": true
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "license": "MIT",
  "author": "janryWang"
}
